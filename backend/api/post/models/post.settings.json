{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "likes": {
      "default": 0,
      "type": "integer"
    },
    "content": {
      "type": "richtext"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "posts"
    }
  }
}
